<?xml version="1.0" encoding="UTF-8"?>
<Network xmlns="http://www.xmos.com"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://www.xmos.com http://www.xmos.com">
  <Type>Board</Type>
  <Name>PoE Audio 1V0</Name>

  <Declarations>
    <Declaration>core stdcore[1]</Declaration>
  </Declarations>

  <Nodes>
    <Node Id="0" Type="XS1-L1A" Oscillator="25Mhz" SystemFrequency="500MHz">
      <Boot>
        <Source Location="SPI:bootFlash"/>
      </Boot>
      <Core Number="0" Reference="stdcore[0]">
	  
		<!-- ETHERNET -->
		<Port Location="XS1_PORT_1G" Name="PORT_ETH_RXCLK"/>
		<Port Location="XS1_PORT_1H" Name="PORT_ETH_RXER"/>
		<Port Location="XS1_PORT_1I" Name="PORT_ETH_TXCLK"/>
		<Port Location="XS1_PORT_1L" Name="PORT_ETH_RXDV"/>
		<Port Location="XS1_PORT_1J" Name="PORT_ETH_TXEN"/>
		<Port Location="XS1_PORT_1P" Name="PORT_ETH_MDIO"/>
		<Port Location="XS1_PORT_1K" Name="PORT_ETH_MDC"/>
		<Port Location="XS1_PORT_4D" Name="PORT_ETH_RXD"/>
		<Port Location="XS1_PORT_4E" Name="PORT_ETH_TXD"/>
		
		<!-- SHARED & LEDS -->
		<Port Location="XS1_PORT_4C" Name="PORT_SHARED_IN"/>
		<Port Location="XS1_PORT_4A" Name="PORT_SHARED_OUT"/>
		<Port Location="XS1_PORT_4B" Name="PORT_LEDS"/>
		
		<!-- I2C -->
		<Port Location="XS1_PORT_1M" Name="PORT_I2C_SDA"/>
		<Port Location="XS1_PORT_1N" Name="PORT_I2C_SCL"/>    

		<!-- SPI FLASH -->
        <Port Location="XS1_PORT_1A" Name="PORT_SPI_MISO"/> <!-- AMP_LRCK -->
        <Port Location="XS1_PORT_1B" Name="PORT_SPI_SS"/>
        <Port Location="XS1_PORT_1C" Name="PORT_SPI_CLK"/>	<!-- AMP_SCLK -->
        <Port Location="XS1_PORT_1D" Name="PORT_SPI_MOSI"/>	<!-- AMP_SDIN -->
		
		<!-- OTHER -->
		<Port Location="XS1_PORT_1E" Name="PORT_MCLK_IN"/>
		<Port Location="XS1_PORT_1F" Name="PORT_OPTICAL_RX"/> 
		<Port Location="XS1_PORT_1O" Name="PORT_PLL_SYNC_OUT"/> 

      </Core>
    </Node>
  </Nodes>
  

  <Links>
    <Link Encoding="2wire" Delays="4,4" Flags="SOD">
      <LinkEndpoint NodeId="0" Link="X0LA"/>
      <LinkEndpoint RoutingId="0x8000" Chanend="1"/>
    </Link>
  </Links>


  <Packages>
    <Package id="0" Type="XS1-L1A-LQ64">
      <Component NodeId="0" InPackage="0"/>
    </Package>
  </Packages>

  <Links>
  </Links>

  <ExternalDevices>
    <Device NodeId="0" Core="0" Class="SPIFlash" Name="bootFlash" Type="winbond_25x40">
      <Attribute Name="PORT_SPI_MISO" Value="PORT_SPI_MISO"/>
      <Attribute Name="PORT_SPI_SS"   Value="PORT_SPI_SS"/>
      <Attribute Name="PORT_SPI_CLK"  Value="PORT_SPI_CLK"/>
      <Attribute Name="PORT_SPI_MOSI" Value="PORT_SPI_MOSI"/>
    </Device>
  </ExternalDevices>

  <JTAGChain>
     <JTAGDevice NodeId="0"/>
  </JTAGChain>

</Network>


